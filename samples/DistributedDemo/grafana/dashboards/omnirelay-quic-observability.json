      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        { "expr": "sum(rate(omnirelay_http_client_fallbacks_total[5m]))", "legendFormat": "HTTP client fallbacks/s", "refId": "A" },
