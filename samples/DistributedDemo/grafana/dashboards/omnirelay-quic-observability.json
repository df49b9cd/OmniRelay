      "title": "Requests completed by protocol (5m)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": { "defaults": { "custom": {}, "links": [] }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum by(le, network_protocol_version) (rate(omnirelay_http_request_duration_bucket[5m])))", "legendFormat": "p95 HTTP/{{network_protocol_version}}", "refId": "A" }
      ],
      "title": "Request latency p95 by protocol (ms)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": { "defaults": { "custom": {}, "links": [] }, "overrides": [] },
