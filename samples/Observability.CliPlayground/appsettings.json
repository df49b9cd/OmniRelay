{
  "playground": {
    "initialDelay": "00:00:04",
    "scriptInterval": "00:00:20"
  },
  "omnirelay": {
    "service": "samples.observability-cli",
    "diagnostics": {
      "introspection": {
        "enabled": true
      },
      "runtime": {
        "enabled": true
      }
    },
    "transportPolicy": {
      "exceptions": [
        {
          "name": "local-json-http2",
          "category": "diagnostics",
          "appliesTo": [ "diagnostics:http" ],
          "transports": [ "http2" ],
          "encodings": [ "json" ],
          "reason": "Local analysis tools do not yet support HTTP/3",
          "expiresAfter": "2025-12-31T00:00:00Z"
        }
      ]
    },
    "inbounds": {
      "http": [
        {
          "urls": [ "http://0.0.0.0:7130" ]
        }
      ],
      "grpc": [
        {
          "urls": [ "http://0.0.0.0:7131" ]
        }
      ]
    }
  }
}
