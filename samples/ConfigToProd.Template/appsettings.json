{
  "hosting": {
    "urls": "http://0.0.0.0:6050"
  },
  "diagnostics": {
    "runtimeMetricsEnabled": false
  },
  "probes": {
    "readyAfter": "00:00:05",
    "requireDiagnosticsToggle": true
  },
  "omnirelay": {
    "service": "samples.config-to-prod",
    "diagnostics": {
      "runtime": {
        "enabled": false
      },
      "introspection": {
        "enabled": true
      }
    },
    "inbounds": {
      "http": [
        {
          "urls": [ "http://0.0.0.0:7082" ],
          "runtime": {
            "enableHttp3": false
          }
        }
      ],
      "grpc": [
        {
          "urls": [ "http://0.0.0.0:7092" ]
        }
      ]
    },
    "outbounds": {
      "inventory": {
        "unary": {
          "grpc": [
            {
              "service": "inventory",
              "addresses": [ "http://inventory:9090" ],
              "peerChooser": "fewest-pending"
            }
          ]
        }
      }
    },
    "middleware": {
      "inbound": {
        "unary": [ "logging", "tracing", "metrics" ],
        "oneway": [ "logging" ]
      },
      "outbound": {
        "unary": [ "retry" ]
      }
    },
    "encodings": {
      "json": [
        {
          "procedure": "ops::ping",
          "encoding": "application/json"
        },
        {
          "procedure": "ops::heartbeat",
          "encoding": "application/json"
        }
      ]
    }
  }
}
