{
  "hosting": {
    "urls": "http://0.0.0.0:6050"
  },
  "diagnostics": {
    "runtimeMetricsEnabled": false
  },
  "probes": {
    "readyAfter": "00:00:15",
    "requireDiagnosticsToggle": true
  },
  "omnirelay": {
    "diagnostics": {
      "runtime": {
        "enabled": false
      },
      "introspection": {
        "enabled": false,
        "requireApiKey": true
      }
    },
    "inbounds": {
      "http": [
        {
          "urls": [ "http://0.0.0.0:8080" ],
          "runtime": {
            "maxConcurrentRequests": 2048
          }
        }
      ],
      "grpc": [
        {
          "urls": [ "http://0.0.0.0:9090" ]
        }
      ]
    }
  }
}
